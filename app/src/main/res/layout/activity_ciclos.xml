        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/ciclo3"
        app:layout_constraintStart_toEndOf="@+id/ciclo3"
        app:layout_constraintTop_toTopOf="@+id/ciclo3" />

