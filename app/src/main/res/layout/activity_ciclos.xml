        android:text="Esportes aquáticos"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/ciclo5"
        app:layout_constraintStart_toEndOf="@+id/ciclo5"
        app:layout_constraintTop_toTopOf="@+id/ciclo5"
        app:layout_constraintVertical_bias="0.368" />

    <TextView
        android:id="@+id/min8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:alpha="0.7"
        android:text="60 min"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/min7"
